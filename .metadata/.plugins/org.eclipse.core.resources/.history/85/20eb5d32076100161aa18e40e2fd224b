import java.util.Scanner;


public class ArrayTest {
	
	private static Scanner in;

	public static double getAva() {
		int cnt = 0;
		double sum = 0;
		int x = 0;
		double ave = 0.0;
		int[] numbers = new int[100];
		
		in = new Scanner(System.in);
		x = in.nextInt();
		while(x != -1) {
			sum += x;
			x = in.nextInt();
			numbers[cnt++] = x;
		}
		if(cnt != 0) {
			ave = sum / cnt;
			
			for(int i = 0; i <¡¡cnt; i++) {
				if(numbers[i] > ave){
					System.println(numbers[i]);
				}
				
			}
		}
					
		return ave;	
	}
	
}
